// HTMLに残るコメント
//- HTMLに残らないコメント

doctype html
html(lang="ja")
  head
    meta(charset="utf-8")
  body
    h1 #{title}
  section
    // each で app.js内の return res.render('index', { messages: msgs }); でセットしている値をループさせる。
    each msg in messages
      div
        h2 #{msg.username}
        time #{msg.date}
        p #{msg.message}
